<script>
    $("#sscGrade").prop('disabled', true);
    $("#outOfText").prop('disabled', true);
    $("#sscOutOf").prop('disabled', true);

    $("#hscGrade").prop('disabled', true);
    $("#hscOutOfText").prop('disabled', true);
    $("#hscOutOf").prop('disabled', true);

    $("#bscGrade").prop('disabled', true);
    $("#bscOutOfText").prop('disabled', true);
    $("#bscOutOf").prop('disabled', true);

    $("#mscGrade").prop('disabled', true);
    $("#mscOutOfText").prop('disabled', true);
    $("#mscOutOf").prop('disabled', true);

    $("#othersGrade").prop('disabled', true);
    $("#othersOutOfText").prop('disabled', true);
    $("#othersOutOf").prop('disabled', true);


    $("#showOthers").hide();

    $("#showForthExprience").hide();
    $("#showFifthExprience").hide();

    $("#showForthTraining").hide();
    $("#showFifthTraining").hide();



    $(document).ready(function () {

        $('ul.term-list').each(function () {

            var LiN = $(this).find('li').length;

            if (LiN > 2) {
                $('li', this).eq(1).nextAll().hide().addClass('toggleable');
                $(this).append('<li class="more1">more...</li>');
            }

        });


        $('ul.term-list').on('click', '.more1', function () {

            if ($(this).hasClass('less')) {
                $(this).text('more...').removeClass('less');
            } else {
                $(this).text('less...').addClass('less');
            }

            $(this).siblings('li.toggleable').slideToggle();

        });




        //
        var showChar = 200;
        var ellipsestext = "...";
        var moretext = "more";
        var lesstext = "less";
        $('.more').each(function () {
            var content = $(this).html();

            if (content.length > showChar) {

                var c = content.substr(0, showChar);
                var h = content.substr(showChar - 1, content.length - showChar);

                var html = c + '<span class="moreellipses">' + ellipsestext + '&nbsp;</span><span class="morecontent"><span>' + h + '</span>&nbsp;&nbsp;<a href="" class="morelink">' + moretext + '</a></span>';

                $(this).html(html);
            }

        });

        $(".morelink").click(function () {
            if ($(this).hasClass("less")) {
                $(this).removeClass("less");
                $(this).html(moretext);
            } else {
                $(this).addClass("less");
                $(this).html(lesstext);
            }
            $(this).parent().prev().toggle();
            $(this).prev().toggle();
            return false;
        });
        //
        $("#universalSaveButton").click(function () {
            var searchMobileNo = $("#searchMobileNo").val();
            var searchNID = $("#searchNID").val();

            if (searchMobileNo == "") {
                alert("Enter Your Present Mobile No");
                return false;
            };
            if (searchNID == "") {
                alert("Enter Your National Id No");
                return false;
            };

        });

        $("#preferenceAnyWhere").on('change', function () {

            var preferrenceAnyWhere = $(this).is(':checked') ? 1 : 0;
            if (preferrenceAnyWhere == 1) {
                $("#PreferenceNumber1").prop('disabled', true);
                $("#PreferenceNumber2").prop('disabled', true);
                $("#PreferenceNumber3").prop('disabled', true);
                $("#PreferenceNumber1").val("");
                $("#PreferenceNumber2").val("");
                $("#PreferenceNumber3").val("");
            }
            else {
                $("#PreferenceNumber1").prop('disabled', false);
                $("#PreferenceNumber2").prop('disabled', false);
                $("#PreferenceNumber3").prop('disabled', false);
            }
        })


        $("#searchButton").click(function () {
            var searchMobileNo = $("#searchMobileNo").val();
            var searchNID = $("#searchNID").val();

            if (searchMobileNo == "") {
                alert("Enter Your Present Mobile No");
                return false;
            };
            if (searchNID == "") {
                alert("Enter Your National Id No");
                return false;
            };

            $.ajax({                
                url: '@Url.Action("SearchWithMoibleNoAndNID", "Home")',
                type: 'POST',
                data: JSON.stringify({ searchMobileNo: searchMobileNo, searchNID: searchNID }),
                contentType: 'application/json; charset=utf-8',
                dataType: 'json',
                success: function (listOfCandidateInfo) {
                    
                    $("#sourceOfJobInfo").val(listOfCandidateInfo.SourceOfJobInfo);
                    $("#appliedPosition").val(listOfCandidateInfo.AppliedPosition);
                    $("#careerObjective").val(listOfCandidateInfo.CareerObjective);
                    $("#inputName").val(listOfCandidateInfo.ApplicantName);
                    $("#fathersName").val(listOfCandidateInfo.ApplicantFatherName);
                    $("#mothersName").val(listOfCandidateInfo.ApplicantMotherName);                   
                    if (listOfCandidateInfo.DateOfBirth != null) {
                        var customizeDateOfBirth = listOfCandidateInfo.DateOfBirth;
                        var trickStringForDoB = customizeDateOfBirth.substring(6);
                        var trickNumberForDoB = parseInt(trickStringForDoB);
                        var d = new Date(trickNumberForDoB);
                        $("#dateOfBirth").val((d.getMonth() + 1) + "/" + d.getDate() + "/" + d.getFullYear());
                    }

                    $("#ddlGender").val(listOfCandidateInfo.Gender);
                    $("#ddlMaritalStatus").val(listOfCandidateInfo.MaritalStatus);
                    $("#ddlreligion").val(listOfCandidateInfo.Religion);
                    $("#ddlNationality").val(listOfCandidateInfo.Nationality);
                    $("#emailAddress").val(listOfCandidateInfo.EmailAddress);
                    $("#nationalID").val(listOfCandidateInfo.NationalID);                    

                    $("#PreferenceNumber1").val(listOfCandidateInfo.PreferenceNumber1);
                    $("#PreferenceNumber2").val(listOfCandidateInfo.PreferenceNumber2);
                    $("#PreferenceNumber3").val(listOfCandidateInfo.PreferenceNumber3);
                    $("#preferenceAnyWhere").prop('checked', listOfCandidateInfo.PreferenceAnyWhere);
                    if (listOfCandidateInfo.PreferenceAnyWhere == true) {
                        $("#PreferenceNumber1").prop('disabled', true);
                        $("#PreferenceNumber2").prop('disabled', true);
                        $("#PreferenceNumber3").prop('disabled', true);
                    }

                    $("#preHouseNo").val(listOfCandidateInfo.PreHouseNo);
                    $("#preVillageOrStreet").val(listOfCandidateInfo.PreVillageOrStreet);
                    $("#prePostOffice").val(listOfCandidateInfo.PrePostOffice);
                    $("#prePoliceStation").val(listOfCandidateInfo.PrePoliceStation);
                    $("#preDivision").val(listOfCandidateInfo.PreDivision);
                    loadPresentDistrict(listOfCandidateInfo.PreDivision);
                    setTimeout(function () {
                        $("#preDistrict").val(listOfCandidateInfo.PreDistrict);
                    }, 500);
                    loadPresentUpazila(listOfCandidateInfo.PreDistrict);
                    setTimeout(function () {
                        $("#preUpozila").val(listOfCandidateInfo.PreUpozila);
                    }, 500);
                    loadPresentUnion(listOfCandidateInfo.PreUpozila);
                    setTimeout(function () {
                        $("#presentUnionID").val(listOfCandidateInfo.PresentUnionID);
                    }, 500);
                    $("#preMobileNo").val(listOfCandidateInfo.PreMobileNo);
                    $("#preHomePhoneNo").val(listOfCandidateInfo.PreHomePhoneNo);



                    $("#perHouseNo").val(listOfCandidateInfo.PerHouseNo);
                    $("#perVillageOrStreet").val(listOfCandidateInfo.PerVillageOrStreet);
                    $("#perPostOffice").val(listOfCandidateInfo.PerPostOffice);
                    $("#perPoliceStation").val(listOfCandidateInfo.PerPoliceStation);
                    $("#perDivision").val(listOfCandidateInfo.PerDivision);
